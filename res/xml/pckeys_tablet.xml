<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="10%"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

  <Row>
    <Key android:codes="111" android:keyLabel="ESC" android:keyEdgeFlags="left"/>
    <Key android:codes="131" android:keyLabel="F1"/>
    <Key android:codes="132" android:keyLabel="F2"/>
    <Key android:codes="133" android:keyLabel="F3"/>
    <Key android:codes="134" android:keyLabel="F4"/>
    <Key android:codes="135" android:keyLabel="F5"/>
    <Key android:codes="136" android:keyLabel="F6"/>
    <Key android:codes="137" android:keyLabel="F7"/>
    <Key android:codes="138" android:keyLabel="F8"/>
    <Key android:codes="139" android:keyLabel="F9" android:keyEdgeFlags="right"/>
  </Row>
  
  <Row>
    <Key android:codes="29" android:keyLabel="tab" android:isRepeatable="true" android:keyWidth="20%"/>
    <Key android:codes="140" android:keyLabel="F10"/>
    <Key android:codes="141" android:keyLabel="F11"/>
    <Key android:codes="142" android:keyLabel="F12"/>
    <Key android:codes="67" android:keyIcon="@drawable/sym_keyboard_delete" 
         android:keyWidth="15%" android:isRepeatable="true"/>
    <Key android:codes="66" android:keyIcon="@drawable/sym_keyboard_return" 
         android:isRepeatable="true" android:keyWidth="35%" android:keyEdgeFlags="right"/>
  </Row>
  
  <Row>
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" 
         android:keyWidth="20%" android:isModifier="true"
         android:isSticky="true"  android:isRepeatable="true" android:keyEdgeFlags="left"/>
    <Key android:codes="62" android:keyIcon="@drawable/sym_keyboard_space" 
         android:keyWidth="35%" android:isRepeatable="true"/>
    <Key android:codes="47" android:keyLabel="Home"  android:isRepeatable="true" android:keyWidth="15%"/>
    <Key android:codes="19" android:keyIcon="@drawable/sym_keyboard_up"  android:isRepeatable="true" android:keyWidth="15%"/>
    <Key android:codes="39" android:keyLabel="End"  android:isRepeatable="true" android:keyWidth="15%"/>
  </Row>
  
  <Row android:rowEdgeFlags="bottom">
    <Key android:codes="113" android:keyLabel="ctrl" android:keyWidth="20%" android:isRepeatable="true"
         android:keyEdgeFlags="left"/>
    <Key android:codes="57" android:keyLabel="alt"  android:isRepeatable="true" android:keyWidth="20%"/>
    <Key android:codes="-3" android:keyIcon="@drawable/sym_keyboard_done" 
         android:keyWidth="15%"/>
    <Key android:codes="21" android:keyIcon="@drawable/sym_keyboard_left" android:isRepeatable="true" android:keyWidth="15%"/>
    <Key android:codes="20" android:keyIcon="@drawable/sym_keyboard_down" android:isRepeatable="true" android:keyWidth="15%"/>
    <Key android:codes="22" android:keyIcon="@drawable/sym_keyboard_right" android:isRepeatable="true" android:keyWidth="15%" android:keyEdgeFlags="right"/>
  </Row>
</Keyboard>
